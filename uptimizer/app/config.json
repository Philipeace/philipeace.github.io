{
    "global_settings": {
        "check_interval_seconds": 30,
        "check_timeout_seconds": 10
    },
    "clients": {
        "default_client": {
            "settings": {
                "name": "Default Client",
                "disable_floating_elements": false
            },
            "endpoints": [
                {
                    "id": "local-test-ok",
                    "name": "Local Test Server (OK)",
                    "url": "http://uptimizer_test_server:8080/ok",
                    "group": "Test Environment",
                    "check_interval_seconds": 15,
                    "check_timeout_seconds": 5
                },
                {
                    "id": "local-test-fail",
                    "name": "Local Test Server (Fail)",
                    "url": "http://uptimizer_test_server:8080/fail",
                    "group": "Test Environment"
                },
                {
                    "id": "local-test-slow",
                    "name": "Local Test Server (Slow)",
                    "url": "http://uptimizer_test_server:8080/slow",
                    "group": "Test Environment"
                },
                {
                    "id": "external-example",
                    "name": "External Example Site",
                    "url": "https://example.com",
                    "group": "External Services",
                    "check_interval_seconds": 60
                }
            ]
        },
        "another_client": {
            "settings": {
                "name": "Another Client",
                "disable_floating_elements": false
            },
            "endpoints": [
                {
                    "id": "google-dns",
                    "name": "Google DNS Check",
                    "url": "https://dns.google/",
                    "group": "Infra"
                }
            ]
        }
    }
}